{
    "type": "origins:multiple",
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 300,
        "hud_render": {
            "should_render": false
        }
    },
    "tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:chance",
                    "action": {
                        "type": "origins:change_resource",
                        "resource": "*:*_resource",
                        "change": 1
                    },
                    "chance": 0.8
                }
            ]
        },
        "interval": 4,
        "falling_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "loot spawn ~ ~ ~ loot merchantpug:gameplay/burley"
                },
                {
                    "type": "origins:area_of_effect",
                    "radius": 0.1,
                    "bientity_action": {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:add_velocity",
                            "y": 0.3,
                            "space": "world"
                        }
                    },
                    "bientity_condition": {
                        "type": "origins:target_condition",
                        "condition": {
                            "type": "origins:entity_type",
                            "entity_type": "minecraft:item"
                        }
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "kill @s"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.fish.swim neutral @a ~ ~ ~ 0.8 1"
                },
                {
                    "type": "origins:delay",
                    "action": {
                        "type": "origins:execute_command",
                        "command": "playsound minecraft:entity.fish.swim neutral @a ~ ~ ~ 0.8 1.25"
                    },
                    "ticks": 20
                },
                {
                    "type": "origins:delay",
                    "action": {
                        "type": "origins:execute_command",
                        "command": "playsound minecraft:entity.fish.swim neutral @a ~ ~ ~ 1 1.25"
                    },
                    "ticks": 40
                }
            ]
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_resource",
            "comparison": "<",
            "compare_to": 300
        }
    },
    "splash_sounds": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:if_else_list",
            "actions": [
                {
                    "condition": {
                        "type": "origins:resource",
                        "resource": "*:*_resource",
                        "comparison": "<",
                        "compare_to": 100
                    },
                    "action": {
                        "type": "origins:chance",
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:spawn_particles",
                                    "particle": {
                                        "type": "minecraft:splash"
                                    },
                                    "count": 4,
                                    "speed": 1.0,
                                    "force": false,
                                    "spread": {
                                        "x": 2.5,
                                        "y": 0.0,
                                        "z": 2.5
                                    }
                                },
                                {
                                    "type": "origins:execute_command",
                                    "command": "playsound minecraft:entity.generic.splash neutral @a ~ ~ ~ 0.4 1.6"
                                }
                            ]
                        },
                        "chance": 0.4
                    }
                },
                {
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:resource",
                                "resource": "*:*_resource",
                                "comparison": ">",
                                "compare_to": 100
                            },
                            {
                                "type": "origins:resource",
                                "resource": "*:*_resource",
                                "comparison": "<=",
                                "compare_to": 200
                            }
                        ]
                    },
                    "action": {
                        "type": "origins:chance",
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:spawn_particles",
                                    "particle": {
                                        "type": "minecraft:splash"
                                    },
                                    "count": 16,
                                    "speed": 1.0,
                                    "force": false,
                                    "spread": {
                                        "x": 5.0,
                                        "y": 0.0,
                                        "z": 5.0
                                    }
                                },
                                {
                                    "type": "origins:execute_command",
                                    "command": "playsound minecraft:entity.generic.splash neutral @a ~ ~ ~ 0.4 1.6"
                                }
                            ]
                        },
                        "chance": 0.6
                    }
                },
                {
                    "condition": {
                        "type": "origins:resource",
                        "resource": "*:*_resource",
                        "comparison": ">",
                        "compare_to": 200
                    },
                    "action": {
                        "type": "origins:chance",
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:spawn_particles",
                                    "particle": {
                                        "type": "minecraft:splash"
                                    },
                                    "count": 16,
                                    "speed": 1.0,
                                    "force": false,
                                    "spread": {
                                        "x": 5.0,
                                        "y": 0.0,
                                        "z": 5.0
                                    }
                                },
                                {
                                    "type": "origins:execute_command",
                                    "command": "playsound minecraft:entity.generic.splash neutral @a ~ ~ ~ 0.4 1.6"
                                }
                            ]
                        },
                        "chance": 0.9
                    }
                }
            ]
        },
        "interval": 12
    }
}