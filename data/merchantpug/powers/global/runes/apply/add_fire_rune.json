{
    "type": "origins:multiple",
    "tool": {
        "type": "origins:item_on_item",
        "using_item_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:ingredient",
                    "ingredient": {
                        "item": "minecraft:knowledge_book"
                    }
                },
                {
                    "type": "origins:nbt",
                    "nbt": "{CustomModelData:416516}"
                }
            ]
        },
        "on_item_condition": {
            "type": "origins:and",
            "conditions": [
                { 
                    "type": "origins:ingredient",
                    "ingredient": {
                        "tag": "merchantpug:tools"
                    }
                },
                {
                    "type": "origins:nbt",
                    "nbt": "{Powers:[{Power: \"merchantpug:global/runes/effect/fire/fire_rune_tool\"}]}",
                    "inverted": true
                }
            ]
        },
        "using_item_action": {
            "type": "origins:modify",
            "modifier": "ioi-pi:api/select_extra_item"
        },
        "on_item_action": {
            "type": "origins:modify",
            "modifier": "ioi-pi:api/select_item"
        },
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tag @s add merchant.apply_fire_rune_tool"
                },
                {
                    "type": "origins:execute_command",
                    "command": "function #ioi-pi:api/selected_item/get"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @s remove merchant.apply_fire_rune_tool"
                }
            ]
        }
    },
