{
    "type": "origins:multiple",
    "emerald_rune": {
        "type": "origins:item_on_item",
        "using_item_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:ingredient",
                    "ingredient": {
                        "item": "minecraft:knowledge_book"
                    }
                },
                {
                    "type": "origins:nbt",
                    "nbt": "{CustomModelData:416514}"
                }
            ]
        },
        "on_item_condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:nbt",
                    "nbt": "{Powers:[{Power: \"merchantpug:global/runes/effect/emerald/emerald_rune_tool\"}]}"
                },
                {
                    "type": "origins:nbt",
                    "nbt": "{Powers:[{Power: \"merchantpug:global/runes/effect/emerald/emerald_rune_head\"}]}"
                },
                {
                    "type": "origins:nbt",
                    "nbt": "{Powers:[{Power: \"merchantpug:global/runes/effect/emerald/emerald_rune_chest\"}]}"
                },
                {
                    "type": "origins:nbt",
                    "nbt": "{Powers:[{Power: \"merchantpug:global/runes/effect/emerald/emerald_rune_legs\"}]}"
                },
                {
                    "type": "origins:nbt",
                    "nbt": "{Powers:[{Power: \"merchantpug:global/runes/effect/emerald/emerald_rune_feet\"}]}"
                }
            ]
        },
        "using_item_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:modify",
                    "modifier": "ioi-pi:api/select_extra_item"
                },
                {
                    "type": "origins:modify",
                    "modifier": "merchantpug:set_emerald_rune"
                }
            ]
        },
        "on_item_action": {
            "type": "origins:modify",
            "modifier": "ioi-pi:api/select_item"
        },
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tag @s add merchant.apply_rune_essence"
                },
                {
                    "type": "origins:execute_command",
                    "command": "function #ioi-pi:api/selected_item/get"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @s remove merchant.apply_rune_essence"
                }
            ]
        }
    },
